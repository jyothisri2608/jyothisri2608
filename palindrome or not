#include<stdio.h>
#include<string.h>
    void main()
    {

    char a[20];
    printf("enter any word:");
    gets(a);
    int j;
    int len=strlen(a);
    for(int i=0,j=len-1;i<j;i++,j--)
    {
        if(a[i]!=a[j])
        {
             printf("not a palindrome");
          return;
        }

    }
    printf("palindrome");


}
enter any word:amma
palindrome
