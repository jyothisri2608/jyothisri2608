#include<stdio.h>
#include<string.h>
void main()
{
    char pass[40];
    printf("enter the password:");
    scanf("%s",pass);
    int len=strlen(pass);
    int l=0,u=0,d=0,s=0;
    if (len>7)
    {
        for(int i=0;i<len;i++)
      {
        if(islower (pass[i]))
            l++;
        else if (isupper (pass[i]))
            u++;
        else if (isdigit (pass[i]))
            d++;
        else
            s++;
      }
      if(l>0&&u>0&&d>0&&s>0)

          printf("strong password");
      else
          printf("weak");
    }
    else
    printf("weak due to less characters");

}
enter the password:raju@RAJU
weak
enter the password:rajuRaju@123
strong password


